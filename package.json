{
  "name": "gatsby-starter-tailwind",
  "description": "Gatsby starter styled with Tailwind",
  "version": "4.0.0",
  "author": "Taylor Bryant <taylor@bryant.io>",
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "core-js": "^3.22.8",
    "gatsby": "^2.23.18",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-google-analytics": "^2.3.5",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-hotjar": "^1.1.1",
    "gatsby-plugin-manifest": "2.4.10",
    "gatsby-plugin-mdx": "^2.6.0",
    "gatsby-plugin-offline": "3.2.8",
    "gatsby-plugin-postcss": "2.3.2",
    "gatsby-plugin-react-helmet": "3.3.3",
    "gatsby-plugin-sharp": "2.6.10",
    "gatsby-plugin-sitemap": "^2.4.7",
    "gatsby-remark-copy-linked-files": "^2.3.11",
    "gatsby-remark-images": "^3.3.20",
    "gatsby-remark-smartypants": "^2.3.10",
    "gatsby-source-filesystem": "^2.3.22",
    "gatsby-transformer-sharp": "^2.5.11",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "react-slick": "^0.28.1",
    "remark-slug": "^5.1.2",
    "slick-carousel": "^1.8.1"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "9.8.0",
    "babel-eslint": "10.1.0",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.4",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-react": "7.20.0",
    "gatsby-plugin-eslint": "2.0.8",
    "prettier": "2.0.5",
    "tailwindcss": "1.4.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind"
  },
  "bugs": {
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind/issues"
  }
}
